public class P10 {
	public static void main(String[] args) {
		Carrrrr car1;
		car1 = new Carrrrr();
		
		car1.num = 1234;
		car1.gas = 20.5;	
		
		car1.show();
		car1.show();
	}
}

class Carrrrr
{
	//Car類別
	int num;
	double gas;
	
	void show()
	{
		System.out.println("1車號是" + num);
		System.out.println("1油量是" + gas);
	}
}