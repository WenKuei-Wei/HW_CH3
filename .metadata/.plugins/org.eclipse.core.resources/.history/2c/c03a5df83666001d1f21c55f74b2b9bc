public class P14 {
	public static void main(String[] args) {
		Cara car1;
		car1 = new Cara();
		
		car1.setnum(1234);
		car1.setgas(20.5)
	
		
	}
}

class Cara
{
	//Car類別
	int num;
	double gas;
	
	void setnum(int n)
	{
		num = n;
		System.out.println("車號是" + num);
	}
	
	void setgas(int i)
	{
		gas = i;
		System.out.println("油量是" + gas);		
	}
}